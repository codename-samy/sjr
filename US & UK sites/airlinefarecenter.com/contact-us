<script>
        $(function()
        {
            $("#txtName").alphanum({
                allowCaseless: false,
                allowLatin: true,
                allowOtherCharSets: false,
                allowNumeric: false,
                allowNewline: false,
            });
            $('#txtMobile').numeric({
                allowPlus: false,
                allowMinus: false,
                allowThouSep: false,
                allowDecSep: false,
                allowLeadingSpaces: false,
            });

        })
</script>
        <script type="text/javascript">

            function Close() {
                $("#thanksmsg").hide();
                $("#lblMsg").hide();
                document.getElementById("txtName").focus();
            }

            function ResetForm() {
                $("#error-name,#error-email,#error-comment").html("");
                document.getElementById("txtName").value = "";
                document.getElementById("txtEmail").value = "";
                document.getElementById("txtMessage").value = "";
                document.getElementById("txtMobile").value = "";
                document.getElementById("txtName").focus();
            }

            function validateContactForm() {
                var isValid = true;

                $("#error-name,#error-email,#error-phone,#error-comment").html("");
                var name = document.getElementById("txtName");
                var email = document.getElementById("txtEmail");
                var mobile = document.getElementById("txtMobile");
                var message = document.getElementById("txtMessage");
                if (name.value.trim() == "") {
                    $("#error-name").html("Please enter your name.");
                    document.getElementById("txtName").focus();
                }
                else if (name.value.trim() != "" && CheckValidName(name.value.trim()) == false) {
                    $("#error-name").html("Please enter valid name.");
                    document.getElementById("txtName").focus();
                  
                }
                if (email.value.trim() == "") {
                    $("#error-email").html("Please enter your email.");

                }
                if (email.value.trim() != "" && ValidateEmail(email.value.trim()) == false) {
                    $("#error-email").html("Please enter valid email.");

                }
                if (mobile.value.trim() == "") {
                    $("#error-phone").html("Please enter mobile number.");

                }
                if (mobile.value.trim() != "" && CheckValidPhoneNumber(mobile.value.trim()) == false) {
                    $("#error-phone").html("Please enter valid mobile number.");

                }
                if (message.value.trim() == "") {
                    $("#error-comment").html("Please enter your comments.");

                }

                if (name.value.trim().length == 0 || email.value.trim().length == 0 || mobile.value.trim().length == 0 || message.value.trim().length == 0 || ValidateEmail(email.value.trim()) == false || CheckValidPhoneNumber(mobile.value.trim()) == false) {
                    isValid = false;
                }

                if (name.value.trim().length == 0) {
                    document.getElementById("txtName").focus();
                    isValid = false;
                    return isValid;
                }
                else if (name.value.trim() != "" && CheckValidName(name.value.trim()) == false) {
                    isValid = false;
                    document.getElementById("txtName").focus();
                    return isValid;
                }
                else if (fnCheckForRestrictedWords("txtName") == false) {
                    isValid = false;
                    $("#error-name").html("You have entered some restricted words.");
                    return isValid;
                }
                else if (email.value.trim().length == 0 || ValidateEmail(email.value.trim()) == false) {
                    isValid = false;
                    document.getElementById("txtEmail").focus();
                    return isValid;
                }
                else if (mobile.value.trim().length == 0 || CheckValidPhoneNumber(mobile.value.trim()) == false) {
                    isValid = false;
                    document.getElementById("txtMobile").focus();
                    return isValid;
                }
                else if (fnCheckForRestrictedWords("txtMessage") == false) {
                    isValid = false;
                    $("#error-comment").html("You have entered some restricted words.");
                    return isValid;
                }
                else if (message.value.trim().length == 0)
                {
                    isValid = false;
                    document.getElementById("txtMessage").focus();
                    return isValid;
                }

                return isValid;
            }

            $("#txtName").on('input', function () {
                $("#error-name").html("");
                var name = document.getElementById("txtName")
                if (name.value.trim().length == 0) {
                    $("#error-name").html("Please enter your name.");
                }
                else if (name.value.trim() != "" && CheckValidName(name.value.trim()) == false) {
                    $("#error-name").html("Please enter valid name.");
                   
                }
                else if (fnCheckForRestrictedWords("txtName") == false) {
                    $("#error-name").html("You have entered some restricted words.")
                }
            });
            $("#txtEmail").on('input', function () {
                $("#error-email").html("");
                var email = document.getElementById("txtEmail");
                if (email.value.trim().length == 0) {
                    $("#error-email").html("Please enter your email.");

                }
                else if (ValidateEmail(email.value.trim()) == false) {
                    $("#error-email").html("Please enter valid email.")
                }
            });
            $("#txtMobile").on('input', function () {
                $("#error-phone").html("");
                var mobile = document.getElementById("txtMobile");
                if (mobile.value.trim().length == 0) {
                    $("#error-phone").html("Please enter mobile number.");

                }
                else if (CheckValidPhoneNumber(mobile.value.trim()) == false) {
                    $("#error-phone").html("Please enter valid mobile number.")
                }
            });
            $("#txtMessage").on('input', function () {
                $("#error-comment").html("");
                var message = document.getElementById("txtMessage");
                if (message.value.trim().length == 0) {
                    $("#error-comment").html("Please enter your comments.")
                }
                else if (fnCheckForRestrictedWords("txtMessage") == false) {
                    $("#error-comment").html("You have entered some restricted words.")
                }
            });

            $(function () {
                $("#btnSubmit").click(function (e) {
                    if (validateContactForm()) {
                        $.ajax(
                        {
                            url: "../FooterLinks/ContactUs",
                            type: "POST",
                            data: $("#contactform").serializeArray(),
                            cache: false,
                            beforeSend: function () {
                                $("#btnSubmit").val('Please wait...');
                                $("#btnSubmit").attr('disabled', 'disabled');

                            },
                            success: function (data) {
                                var dataErr = data.split('#');
                        if (dataErr.length > 1) {
                            
                            $("#thanksmsg").html("<span class='wrong-thanks'></span>You have entered some restricted words.<span   onclick='return Close()'>X</span>");
                            $("#thanksmsg").show();
                            $("#btnSubmit").val('Send Message');
                            $("#btnSubmit").removeAttr('disabled');
                            //ResetForm();
                            return false;

                        }
                               
                        $("#thanksmsg").html("<span class='right-thanks'></span>Your message has been sent.<span   onclick='return Close()'>X</span>");
                                $("#thanksmsg").show();
                                $("#btnSubmit").val('Send Message');
                                $("#btnSubmit").removeAttr('disabled');
                                ResetForm();
                            },
                            error: function (err) {
                                $("#btnSubmit").val('Send Message');
                                $("#btnSubmit").removeAttr('disabled');
                               
                                $("#thanksmsg").html("<span class='right-thanks'></span>Your message has been sent.<span   onclick='return Close()'>X</span>");
                                $("#thanksmsg").show();
                                ResetForm();
                            }
                        });
                    }
                });
            });

            function fnCheckForRestrictedWords(inputTxt) {

                var IsValid = true;
                var restrictedWords = new Array("Hot Girls", "Porn", "Sexy Girl", "Hot Babes", "Chicks", "Bikini", "Sexual", "Hard Core", "Hottest", "vampires", "Gambling", "Fetish", "Casino", "Poker", "Lottery", "Bitcoin", "crypto", "Mining", "money online", "XXX", "SEX", "Escort", "free", "boobs", "Dating", "Chat", "WebCam", "Live", "Secret", "Hidden", "Gay", "Join", "SPA", "Massage", "Income", "tits", "fuck", "hardcore", "asian", "HTTP", "WWW.", "href", "HTTPS", "horny", "dick", "penis", "busty");
                var txtInput = $.trim($("#" + inputTxt).val());
                for (var i = 0; i < restrictedWords.length; i++) {
                    var val = restrictedWords[i];
                    if ((txtInput.toLowerCase()).indexOf(val.toLowerCase().toString()) > -1) {
                        IsValid = false;
                    }
                }
                if (inputTxt == 'txtMessage') {
                    var wordsLength = txtInput.split(' ');
                    if (wordsLength.length < 5) {
                        IsValid = false;
                    }
                }
                return IsValid;
            }

            function isNumberKeys(evt) {
                var charCode = (evt.which) ? evt.which : event.keyCode
                if (charCode > 31 && (charCode < 48 || charCode > 57))
                    return false;

                return true;
            }

            function CheckValidName(UName) {
                var IsValid = true;
                if (UName.length < 3) {
                    IsValid = false;
                }
                if (UName.length > 50) {
                    IsValid = false;
                }

                return IsValid;
            }

            function CheckValidPhoneNumber(PhoneNo) {
                var IsValid = true;
                if (PhoneNo.length < 10) {
                    IsValid = false;
                }
                if (startsWith(PhoneNo, "00")) {
                    IsValid = false;
                }
                if (startsWith(PhoneNo, "1111111111")) {
                    IsValid = false;
                }
                if (startsWith(PhoneNo, "2222222222")) {
                    IsValid = false;
                }
                if (startsWith(PhoneNo, "3333333333")) {
                    IsValid = false;
                }
                if (startsWith(PhoneNo, "4444444444")) {
                    IsValid = false;
                }
                if (startsWith(PhoneNo, "5555555555")) {
                    IsValid = false;
                }
                if (startsWith(PhoneNo, "6666666666")) {
                    IsValid = false;
                }
                if (startsWith(PhoneNo, "7777777777")) {
                    IsValid = false;
                }
                if (startsWith(PhoneNo, "8888888888")) {
                    IsValid = false;
                }
                if (startsWith(PhoneNo, "9999999999")) {
                    IsValid = false;
                }
                return IsValid;
            }
            function startsWith(str, word) {
                return str.lastIndexOf(word, 0) === 0;
            }

            //Submit Form Auto Scroll stop
            function removeStuff() {
                var elem = document.getElementById("center");
                if (document.getElementById("container") != null) {
                    document.getElementById("container").removeChild(elem);
                    document.getElementById("container").appendChild(elem);
                }
            }
        </script>
