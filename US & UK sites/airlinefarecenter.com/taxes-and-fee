<script>
    $(document).ready(function () {
        $('.ScrollTop').click(function () {
            $('html, body').animate({ scrollTop: 0 }, 150);
            return false;
        });
        $(window).scroll(function () {
            if ($(this).scrollTop() > 150) {
                $(".ScrollTop").addClass("onscroll");
            } else {
                $(".ScrollTop").removeClass("onscroll");
            }
        });

    });
</script>

