<script>
    var FOBmobileStripWarning_Timer;
    $(function () {
        StartFOBMobileStripTimers();

        $('.callCloseicon').on('click', function () {
            $('.footerCallSection').hide();

            $(this).hide();
            sessionStorage.setItem('fobmobstrippopuphide', "Y");

        });
        $(document).click(function (event) {
            if (!$(event.target).closest(".footerCallSection").length) {
                $("body").find(".footerCallSection").hide();
                $('.callCloseicon').hide();
            }
            if (FOBmobileStripWarning_Timer) {
                clearTimeout(FOBmobileStripWarning_Timer);
                FOBmobileStripWarning_Timer = null;
            }
            StartFOBMobileStripTimers();
        });


        $(window).scroll(function () {                  // assign scroll event listener
            $("body").find(".footerCallSection").hide();
            $('.callCloseicon').hide();
            if (FOBmobileStripWarning_Timer) {
                clearTimeout(FOBmobileStripWarning_Timer);
                FOBmobileStripWarning_Timer = null;
            }
            StartFOBMobileStripTimers();

        });

    });
    function StartFOBMobileStripTimers() {
        FOBmobileStripWarning_Timer = setTimeout(StartFOBMobileStripIdle_Warning, 2000);
    }
    function StartFOBMobileStripIdle_Warning() {
        if (sessionStorage.getItem('fobmobstrippopuphide') == null || sessionStorage.getItem('fobmobstrippopuphide').toString() == "") {
            $('.footerCallSection').show();
	    $('.callCloseicon').show();
            StartFOBMobileStripTimers();
        }

    }
</script>
