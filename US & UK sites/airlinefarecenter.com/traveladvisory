        $(document).ready(function(){
            var oldURL = window.location.href;
            if (history.pushState) {
                var newUrl = oldURL;
                if(newUrl.indexOf('&device=mobile')>0 || newUrl.indexOf('?device=mobile')>0)
                {
                    newUrl =newUrl.replace('&device=mobile','').replace('?device=mobile','');
                }
                if(newUrl.indexOf('?')>0)
                {
                    newUrl +=  "&device=mobile";
                }
                else
                {
                    newUrl += "?device=mobile";

                }
                window.history.pushState({ path: newUrl }, '', newUrl);
            }
            return true;

        });
    </script>
