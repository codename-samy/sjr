          //Keep this code for Reloading on Back Button Click//
          //**** For Chrome/Mozilla***
          if (!!window.performance && window.performance.navigation.type === 2) {
              window.location.reload();
          }
          //**** For Mac - Safari***
          window.onpageshow = function (event) {
              if (event.persisted) {
                  window.location.reload();
              }
          };
          //Keep this code for Reloading on Back Button Click//

        //Menu
          $(function () {
              $('.right-header .toggle-icon').click(function () {
                  $('.header .right-header nav').addClass('active-t');
                  $('.cover').show();
              });
              $('.header .right-header a.menuclose').click(function () {
                  $('.header .right-header nav').removeClass('active-t');
                  $('.cover').hide();
              });
              $('.cover').click(function () {
                  $('.header .right-header nav').removeClass('active-t');
                  $('.cover').hide();
              });
              $('.header a.submenus.dest').click(function () {
                  $('.header ul.submenu.dest').slideToggle();
                  $('.header ul.submenu.thems').slideUp();
                  $('.header ul.submenu.top-deal').slideUp();
                  $('.header ul.submenu.imp-links').slideUp();
                  $('.header ul.submenu.legal-links').slideUp();

                  $('a.submenus.dest').toggleClass('s-menu');
                  $('a.submenus.thems').removeClass('s-menu');
                  $('a.submenus.top-deal').removeClass('s-menu');
                  $('a.submenus.imp-links').removeClass('s-menu');
                  $('a.submenus.legal-links').removeClass('s-menu');
              });
              
              $('.header a.submenus.thems').click(function () {
                  $('.header ul.submenu.thems').slideToggle();
                  $('.header ul.submenu.dest').slideUp();
                  $('.header ul.submenu.top-deal').slideUp();
                  $('.header ul.submenu.imp-links').slideUp();
                  $('.header ul.submenu.legal-links').slideUp();

                  $('a.submenus.thems').toggleClass('s-menu');
                  $('a.submenus.dest').removeClass('s-menu');
                  $('a.submenus.top-deal').removeClass('s-menu');
                  $('a.submenus.imp-links').removeClass('s-menu');
                  $('a.submenus.legal-links').removeClass('s-menu');
              });
              $('.header a.submenus.top-deal').click(function () {
                  $('.header ul.submenu.top-deal').slideToggle();
                  $('.header ul.submenu.thems').slideUp();
                  $('.header ul.submenu.dest').slideUp();
                  $('.header ul.submenu.imp-links').slideUp();
                  $('.header ul.submenu.legal-links').slideUp();

                  $('a.submenus.top-deal').toggleClass('s-menu');
                  $('a.submenus.thems').removeClass('s-menu');
                  $('a.submenus.dest').removeClass('s-menu');
                  $('a.submenus.imp-links').removeClass('s-menu');
                  $('a.submenus.legal-links').removeClass('s-menu');
                  
              });

              $('.header a.submenus.imp-links').click(function () {
                  $('.header ul.submenu.imp-links').slideToggle();
                  $('.header ul.submenu.top-deal').slideUp();
                  $('.header ul.submenu.thems').slideUp();
                  $('.header ul.submenu.dest').slideUp();
                  $('.header ul.submenu.legal-links').slideUp();

                  $('a.submenus.imp-links').toggleClass('s-menu');
                  $('a.submenus.top-deal').removeClass('s-menu');
                  $('a.submenus.thems').removeClass('s-menu');
                  $('a.submenus.dest').removeClass('s-menu');
                  $('a.submenus.legal-links').removeClass('s-menu');

              });
              $('.header a.submenus.legal-links').click(function () {
                  $('.header ul.submenu.legal-links').slideToggle();
                  $('.header ul.submenu.top-deal').slideUp();
                  $('.header ul.submenu.thems').slideUp();
                  $('.header ul.submenu.dest').slideUp();
                  $('.header ul.submenu.imp-links').slideUp();
                 
                  $('a.submenus.legal-links').toggleClass('s-menu');
                  $('a.submenus.top-deal').removeClass('s-menu');
                  $('a.submenus.thems').removeClass('s-menu');
                  $('a.submenus.dest').removeClass('s-menu');
                  $('a.submenus.imp-links').removeClass('s-menu');
                 

              });

          });
        
          $(function () {
              $('.sf-close').click(function () {
                  $('.topsaftytravl').hide();
              });
          });



    </script>
    <script>
    function FOB_CheckCookiesStatus() {

        if (navigator.cookieEnabled) {
            HideCookiesModal();
            getCookiesStatus();
        }
        else {
            CallCookiesModal();
        }
    }

    function IsCookiesEnabled() {
        HideCookiesModal();
        location.reload();
        if (navigator.cookieEnabled) {
            HideCookiesModal();
        }
        else {
            CallCookiesModal();
        }
    }

    function HideCookiesModal() {
        $("#dvCheckCookies").hide();
    }

    function CallCookiesModal() {
        $("#dvCheckCookies").show();
    }

    function CallAcceptPopup() {
        $("#dvAcceptCookies").show();
    }

    function HideAcceptPopup() {
        $("#dvAcceptCookies").hide();
    }

    function CloseAcceptPopup() {
        if (navigator.cookieEnabled) {
            HideAcceptPopup();
            getCookiesStatus();
        }
        else {
            CallAcceptPopup();
        }
    }

    function AllowCookies() {
        if (navigator.cookieEnabled) {

            setCookies();
            HideAcceptPopup();
        }
        else {
            CallAcceptPopup();
        }
    }

    function getCookiesStatus() {

        if (typeof (Storage) !== "undefined") {
            if (navigator.cookieEnabled) {
                if (localStorage.length > 0) {
                    if (localStorage.getItem("FOB_TnC_CookiesData") != null && localStorage.getItem("FOB_TnC_CookiesData") != "") {
                        HideAcceptPopup();
                    }
                    else {
                        CallAcceptPopup();
                    }
                }
                else {
                    CallAcceptPopup();
                }
            }
            else {
                CallCookiesModal();
            }
        }
        else {
            CallCookiesModal();
        }
    }

    function setCookies() {
        var xhttp = new XMLHttpRequest();
        xhttp.onreadystatechange = function () {
            if (this.readyState == 4 && this.status == 200) {
                localStorage.setItem("FOB_TnC_CookiesData", "FOB Term And Condition Cookies Data");
            }
        };
        xhttp.open("GET", "/Flight/SetCookies?Type=SetStatus", true);
        xhttp.send();
    }
    </script>

    <!-- Start of  Zendesk Widget script -->
    
    <!-- End of  Zendesk Widget script -->
    
</body>
</html>